<h1>Listing estado_presupuestos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estado_presupuestos.each do |estado_presupuesto| %>
      <tr>
        <td><%= estado_presupuesto.nombre %></td>
        <td><%= link_to 'Ver', estado_presupuesto %></td>
        <td><%= link_to 'Editar', edit_estado_presupuesto_path(estado_presupuesto) %></td>
        <td><%= link_to 'Borrar', estado_presupuesto, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Estado presupuesto', new_estado_presupuesto_path %>
