<%= simple_form_for(@regional) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :domicilio %>
    <%= f.input :telefono_contacto %>
    <%= f.input :nombre_contacto %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
