<h1>Listing organizacion_externa_proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Organizacion externa</th>
      <th>Proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizacion_externa_proyectos.each do |organizacion_externa_proyecto| %>
      <tr>
        <td><%= organizacion_externa_proyecto.organizacion_externa %></td>
        <td><%= organizacion_externa_proyecto.proyecto %></td>
        <td><%= link_to 'Show', organizacion_externa_proyecto %></td>
        <td><%= link_to 'Edit', edit_organizacion_externa_proyecto_path(organizacion_externa_proyecto) %></td>
        <td><%= link_to 'Destroy', organizacion_externa_proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

