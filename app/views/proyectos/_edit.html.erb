<div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
              <h1>Editing proyecto</h1>
        </div>
        <div class="modal-body">
            <%= render 'edit_form' %>

            <%= link_to 'Show', @proyecto %> |
            <%= link_to 'Back', proyectos_path %>
