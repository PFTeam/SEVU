<h1>Listing tipo_privilegios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_privilegios.each do |tipo_privilegio| %>
      <tr>
        <td><%= tipo_privilegio.nombre %></td>
        <td><%= tipo_privilegio.descripcion %></td>
        <td><%= link_to 'Show', tipo_privilegio %></td>
        <td><%= link_to 'Edit', edit_tipo_privilegio_path(tipo_privilegio) %></td>
        <td><%= link_to 'Destroy', tipo_privilegio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo privilegio', new_tipo_privilegio_path %>
