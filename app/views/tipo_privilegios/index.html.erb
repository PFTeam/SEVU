<!----->
<div class="col-lg-12">
  <h2>Lista de Tipos de Privilegios</h2>
    <table class="table table-hover table-striped tablesorter">
			<thead>
    		<tr>
      		<th>Nombre</th>
      		<th>Descripcion</th>
    		</tr>
  		</thead>
    	<tbody>
       	<% @tipo_privilegios.each do |tipo_privilegio| %>
      		<tr>
       	 		<td><%= tipo_privilegio.nombre %></td>
       	 		<td><%= tipo_privilegio.descripcion %></td>
        		<td><%= link_to 'Editar', edit_tipo_privilegio_path(tipo_privilegio) %></td>
      	 		<td><%= link_to 'Eliminar', tipo_privilegio, method: :delete, data: { confirm: 'Esta seguro de eliminar el tipo de privilegio?' } %></td>
      		</tr>
    		<% end %>
    	</tbody>
    </table>
  </div>
</div>
