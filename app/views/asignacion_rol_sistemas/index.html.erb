<h1>Listing asignacion_rol_sistemas</h1>

<table>
  <thead>
    <tr>
      <th>Fechaasignacion</th>
      <th>Esactual</th>
      <th>Descripcion</th>
      <th>Usuario</th>
      <th>Rol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asignacion_rol_sistemas.each do |asignacion_rol_sistema| %>
      <tr>
        <td><%= asignacion_rol_sistema.fechaAsignacion %></td>
        <td><%= asignacion_rol_sistema.esActual %></td>
        <td><%= asignacion_rol_sistema.descripcion %></td>
        <td><%= asignacion_rol_sistema.usuario %></td>
        <td><%= asignacion_rol_sistema.rol %></td>
        <td><%= link_to 'Show', asignacion_rol_sistema %></td>
        <td><%= link_to 'Edit', edit_asignacion_rol_sistema_path(asignacion_rol_sistema) %></td>
        <td><%= link_to 'Destroy', asignacion_rol_sistema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asignacion rol sistema', new_asignacion_rol_sistema_path %>
