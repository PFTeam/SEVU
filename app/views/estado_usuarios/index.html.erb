<h1>Listing estado_usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estado_usuarios.each do |estado_usuario| %>
      <tr>
        <td><%= estado_usuario.nombre %></td>
        <td><%= link_to 'Ver', estado_usuario %></td>
        <td><%= link_to 'Editar', edit_estado_usuario_path(estado_usuario) %></td>
        <td><%= link_to 'Borrar', estado_usuario, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estado usuario', new_estado_usuario_path %>
