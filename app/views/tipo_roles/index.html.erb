<h1>Listing tipo_roles</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_roles.each do |tipo_rol| %>
      <tr>
        <td><%= tipo_rol.nombre %></td>
        <td><%= tipo_rol.descripcion %></td>
        <td><%= link_to 'Show', tipo_rol %></td>
        <td><%= link_to 'Edit', edit_tipo_rol_path(tipo_rol) %></td>
        <td><%= link_to 'Destroy', tipo_rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo rol', new_tipo_rol_path %>
