<h1>Listing historial_estado_usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Fechacambioestado</th>
      <th>Esactual</th>
      <th>Usuario</th>
      <th>Estado usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historial_estado_usuarios.each do |historial_estado_usuario| %>
      <tr>
        <td><%= historial_estado_usuario.fechaCambioEstado %></td>
        <td><%= historial_estado_usuario.esActual %></td>
        <td><%= historial_estado_usuario.usuario %></td>
        <td><%= historial_estado_usuario.estado_usuario %></td>
        <td><%= link_to 'Show', historial_estado_usuario %></td>
        <td><%= link_to 'Edit', edit_historial_estado_usuario_path(historial_estado_usuario) %></td>
        <td><%= link_to 'Destroy', historial_estado_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historial estado usuario', new_historial_estado_usuario_path %>
