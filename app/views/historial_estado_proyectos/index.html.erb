<h1>Listing historial_estado_proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Fechacambioestado</th>
      <th>Esactual</th>
      <th>Estado proyecto</th>
      <th>Proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historial_estado_proyectos.each do |historial_estado_proyecto| %>
      <tr>
        <td><%= historial_estado_proyecto.fechaCambioEstado %></td>
        <td><%= historial_estado_proyecto.esActual %></td>
        <td><%= historial_estado_proyecto.estado_proyecto %></td>
        <td><%= historial_estado_proyecto.proyecto %></td>
        <td><%= link_to 'Show', historial_estado_proyecto %></td>
        <td><%= link_to 'Edit', edit_historial_estado_proyecto_path(historial_estado_proyecto) %></td>
        <td><%= link_to 'Destroy', historial_estado_proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historial estado proyecto', new_historial_estado_proyecto_path %>
