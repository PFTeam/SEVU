<h1>Listing tipo_notificacions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_notificacions.each do |tipo_notificacion| %>
      <tr>
        <td><%= tipo_notificacion.nombre %></td>
        <td><%= link_to 'Show', tipo_notificacion %></td>
        <td><%= link_to 'Edit', edit_tipo_notificacion_path(tipo_notificacion) %></td>
        <td><%= link_to 'Destroy', tipo_notificacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo notificacion', new_tipo_notificacion_path %>
