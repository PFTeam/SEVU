<h1>Listing tipo_notificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_notificaciones.each do |tipo_notificacion| %>
      <tr>
        <td><%= tipo_notificacion.nombre %></td>
        <td><%= link_to 'Ver', tipo_notificacion %></td>
        <td><%= link_to 'Editar', edit_tipo_notificacion_path(tipo_notificacion) %></td>
        <td><%= link_to 'Borrar', tipo_notificacion, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo notificacion', new_tipo_notificacion_path %>
