<%= form_for([@voluntario, @experiencia]) do |f| %>
  <% if @experiencia.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@experiencia.errors.count, "error") %> prohibited this experiencia from being saved:</h2>

      <ul>
      <% @experiencia.errors.full_messages.each do |message| %>
