<h1>Lista de Colaboradores</h1>

<table>
  <thead>
    <tr>
      <th>Organizacion externa</th>
      <th>Proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colaboradores.each do |colaborador| %>
      <tr>
        <td><%= colaborador.organizacion_externa %></td>
        <td><%= colaborador.proyecto %></td>
        <td><%= link_to 'Ver', colaborador %></td>
        <td><%= link_to 'Editar', edit_colaborador_path(colaborador) %></td>
        <td><%= link_to 'Borrar', colaborador, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Colaborador', new_colaborador_path %>
