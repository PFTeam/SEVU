<div class="modal-dialog">
       <% flash.each do |key, value| %>
  <div class="alert alert-<%= key %> alert-dismissible">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <%= value %>
   </div>
<% end %>
	<div class="modal-content">
	      
	      <div class="modal-header">
	
		<h2>Darse de alta como voluntario</h2>

	</div>
	<div class="modal-body">
		<div class="alert alert-info"><strong>¡Atención!</strong> Una vez que esté dado de alta como voluntario, su condición o legajo no pueden ser cambiados.</div>
		<!-- Renderizado del formulario -->
		<%= render 'dar_alta_voluntario' %>
		<%#= link_to 'Ver', @usuario %> 
		<%#= link_to 'Atrás', :back %>
	</div>







</div><!-- /.modal-content -->

</div><!-- /.modal-dialog -->