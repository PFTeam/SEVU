<h1>Listing horario_disponibles</h1>

<table>
  <thead>
    <tr>
      <th>Diasemana</th>
      <th>Horadesde</th>
      <th>Horahasta</th>
      <th>Voluntario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horario_disponibles.each do |horario_disponible| %>
      <tr>
        <td><%= horario_disponible.diaSemana %></td>
        <td><%= horario_disponible.horaDesde %></td>
        <td><%= horario_disponible.horaHasta %></td>
        <td><%= horario_disponible.voluntario %></td>
        <td><%= link_to 'Show', horario_disponible %></td>
        <td><%= link_to 'Edit', edit_horario_disponible_path(horario_disponible) %></td>
        <td><%= link_to 'Destroy', horario_disponible, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horario disponible', new_horario_disponible_path %>
