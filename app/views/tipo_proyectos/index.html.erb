<h1>Listing tipo_proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_proyectos.each do |tipo_proyecto| %>
      <tr>
        <td><%= tipo_proyecto.nombre %></td>
        <td><%= tipo_proyecto.descripcion %></td>
        <td><%= link_to 'Ver', tipo_proyecto %></td>
        <td><%= link_to 'Editar', edit_tipo_proyecto_path(tipo_proyecto) %></td>
        <td><%= link_to 'Borrar', tipo_proyecto, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo proyecto', new_tipo_proyecto_path %>
