<h1>Listing informe_gastos</h1>

<table>
  <thead>
    <tr>
      <th>Montototal</th>
      <th>Proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @informe_gastos.each do |informe_gasto| %>
      <tr>
        <td><%= informe_gasto.montoTotal %></td>
        <td><%= informe_gasto.proyecto %></td>
        <td><%= link_to 'Ver', informe_gasto %></td>
        <td><%= link_to 'Editar', edit_informe_gasto_path(informe_gasto) %></td>
        <td><%= link_to 'Borrar', informe_gasto, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Informe gasto', new_informe_gasto_path %>
