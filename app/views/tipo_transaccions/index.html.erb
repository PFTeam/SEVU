<h1>Listing tipo_transaccions</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_transaccions.each do |tipo_transaccion| %>
      <tr>
        <td><%= tipo_transaccion.descripcion %></td>
        <td><%= link_to 'Show', tipo_transaccion %></td>
        <td><%= link_to 'Edit', edit_tipo_transaccion_path(tipo_transaccion) %></td>
        <td><%= link_to 'Destroy', tipo_transaccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo transaccion', new_tipo_transaccion_path %>
